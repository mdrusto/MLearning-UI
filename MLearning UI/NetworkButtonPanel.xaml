<UserControl x:Class="MLearning_UI.NetworkButtonPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MLearning_UI"
             mc:Ignorable="d" Height="80" Width="150">
    <Grid>
        <Label x:Name="NetworkName" Content="Label" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Width="150"/>
        <Label x:Name="AccuracyLabel" Content="Label" HorizontalAlignment="Left" Margin="0,26,0,0" VerticalAlignment="Top"/>
        <Label x:Name="NetworkSizeLabel" Content="Label" HorizontalAlignment="Left" Margin="0,52,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
